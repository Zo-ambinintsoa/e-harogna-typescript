<%- include('./layouts/header') -%>
<div class="container mb-5">
    <div class="row">
        <div class="col-12 col-md-11 col-lg-7 col-xl-6 mx-auto">
            <div class="app-branding text-center mb-5">
                <a class="app-logo" href="/"><img class="logo-icon me-2" src="/assets/images/logo.png" alt="logo"><span class="logo-text">E-Harogna</span></a>

            </div><!--//app-branding-->
            <div class="app-card p-5 text-center shadow-sm">
                <h1 class="page-title mb-4">404<br><span class="font-weight-light">Page Not Found</span></h1>
                <div class="mb-4">
                    <% if (typeof error!== 'undefined') { %>
                        <%= error %>
                    <% } else { %>
                        The page you were trying to reach doesn't exist.
                    <% } %>
                </div>
                <a class="btn app-btn-primary" href="/">Go to home page</a>
            </div>
        </div><!--//col-->
    </div><!--//row-->
</div>
<!--//container-->
<footer class="app-footer">
    <div class="container text-center py-3">
        <small class="copyright">Designed by <a class="app-link" href="http://linkedin.com/in/zo-ambinintsoa" target="_blank">Zo</a> for E-Harogna</small>
    </div>
</footer>

<%- include('./layouts/footer') -%>