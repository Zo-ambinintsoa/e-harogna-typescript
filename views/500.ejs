<%- include('./layouts/header') -%>
<div class="container mb-5">
    <div class="row">
        <div class="col-12 col-md-11 col-lg-7 col-xl-6 mx-auto">
            <div class="app-branding text-center mb-5">
                <a class="app-logo" href="/"><img class="logo-icon me-2" src="assets/images/logo.png" alt="logo"><span class="logo-text">E-Harogna</span></a>

            </div><!--//app-branding-->
            <div class="app-card p-5 text-center shadow-sm">
                <h1 class="page-title mb-4">500<br><span class="font-weight-light">Internal Server Error</span></h1>
                <div class="mb-4">
                    <h2>
                        Something isn't right here.
                    </h2>
                    <% if (typeof error !== 'undefined') { %>
                        <div class="error-details mt-2">
                            <pre><code><%- error %></code></pre>
                        </div>
                    <% } else { %>
                        <p class="error">
                            A team of highly trained sea bass is working on this as we speak.<br/>
                            If the problem persists, please <a href="/">contact the system administrator</a> and inform them of the time that the error occured, and anything you might have done that may have caused the error.
                        </p>
                    <% } %>
                </div>
                <a class="btn btn-primary" href="/">Go to home page</a>
            </div>
        </div><!--//col-->
    </div><!--//row-->
</div>
<!--//container-->
<footer class="app-footer">
    <div class="container text-center py-3">
        <small class="copyright">Designed by <a class="app-link" href="http://linkedin.com/in/zo-ambinintsoa" target="_blank">Zo</a> for E-Harogna</small>
    </div>
</footer>
<%- include('./layouts/footer') -%>
