				<div class="tab-content" id="orders-table-tab-content">

			        <div class="tab-pane fade show active" id="orders-all" role="tabpanel" aria-labelledby="orders-all-tab">
					    <div class="app-card app-card-orders-table shadow-sm mb-5">
						    <div class="app-card-body">
							    <div class="table-responsive">
							        <table class="table app-table-hover mb-0 text-left">
										<thead>
											<tr>
												<th class="cell">Ref</th>
												<th class="cell">Titre</th>
												<th class="cell">Nom du companie</th>
												<th class="cell">Poste</th>
												<!-- <th class="cell">Status</th> -->
												<th class="cell">Salaire</th> 
												<th class="cell">disponibiliter</th> 
												<th class="cell"></th>
											</tr>
										</thead>
										<tbody>
											<% data.forEach((item, i) => { %>
											<tr>
												<td class="cell"><%= item.id %></td>
												<td class="cell"><span class="truncate"><%= item.title %></span></td>
												<td class="cell"><span class="truncate"><%= item.companyName %></span></td>
												<td class="cell"><%= item.JobTitle %></td>
												<td class="cell"><span class="note"><%= item.MinSalary %> Ar - <%= item.MaxSalary %> Ar</span></td>
												<!-- <td class="cell"><span class="badge bg-success">Paid</span></td> -->
												<td class="cell"><%= item.IsOpen %></td>
												<td class="cell">
													<a class="text-secondary mr-3" href="/job/view/<%= item.id %>"><i class="fas fa-eye"></i></a>
													<a class="text-danger mr-3" href="/job/delete?id=<%= item.id %>"><i class="fas fa-trash"></i></a>
													<a class="text-success mr-3" href="/job/edit?id=<%= item.id %>"><i class="fas fa-pencil"></i></a>
												</td>
											</tr>
											<% }) %>
										</tbody>
									</table>
						        </div><!--//table-responsive-->
						       
						    </div><!--//app-card-body-->		
						</div><!--//app-card-->


						<nav class="app-pagination">
							<ul class="pagination justify-content-center">
								<% if (meta.page <= 1) { %>
								<li class="page-item disabled">
									<a class="page-link" href="#" tabindex="-1" aria-disabled="true">Previous</a>
							    </li>
								<% } else { %>
									<li class="page-item">
										<a class="page-link" href="courses?page=<%= (meta.page-1) %>" tabindex="-1">Previous</a>
									</li>									
									<% } %>
								<% for(var i=0; i< meta.last_page; i++) {%>
								<li class="page-item <% if((i+1)==meta.page){%> active <% }%>"><a class="page-link" href="courses?page=<%= (i+1) %>"><%= (i+1) %></a></li>
								<% } %>
								<% if (meta.page >= meta.last_page) { %>
									<li class="page-item disabled">
										<a class="page-link" href="#" tabindex="-1" aria-disabled="true">Next</a>
									</li>
									<% } else { %>
										<li class="page-item">
											<a class="page-link" href="courses?page=<%= (meta.page+1) %>" tabindex="-1">Next</a>
										</li>									
										<% } %>

							</ul>
